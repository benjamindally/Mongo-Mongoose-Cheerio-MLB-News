<div class="header jumbotron text-center">

    <h1>Fantasy MLB News</h1>
    <h3 class="text-center">Read and Take Notes</h3>

</div>

<div class="wrapper text-center">

    <div class="row">
        <div class="col-md-5 mx-auto">

            <p>{{article.title}}</p>
            <p>{{article.paragraph}}</p>
            {{#each article.note}}
            <p>{{this.title}}</p>
            <p>{{this.body}}</p>
            <button type="button" class="btn btn-primary" id="delete_note {{this.id}}">Delete Note</button>
            {{/each}}
        </div>
    </div>
</div>